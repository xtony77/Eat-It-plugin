{
	"manifest_version": 2,
  "name": "Eat it",
  "description": "Eat it plugin.",
  "version": "0.1",
  "icons": { "16": "icon_16.png", "128": "icon_128.png" },

  "background": { "scripts": ["jquery-2.1.0.min.js", "background.js"] },
  "permissions": ["tabs"],

  "browser_action": {
  	"default_icon": "icon_16.png",
  	"default_popup": "popup.html"
  }
}